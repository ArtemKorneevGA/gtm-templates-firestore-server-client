___SANDBOXED_JS_FOR_SERVER___

const claimRequest = require("claimRequest");
const returnResponse = require("returnResponse");
const runContainer = require("runContainer");
const getRequestPath = require("getRequestPath");
const getRequestMethod = require("getRequestMethod");
const getRequestHeader = require("getRequestHeader");
const setResponseHeader = require("setResponseHeader");
const log = require("logToConsole");
const getRequestQueryParameters = require("getRequestQueryParameters");
const decodeUriComponent = require("decodeUriComponent");
const getType = require("getType");

const version = 1;
const requestPath = getRequestPath();
const queryParameters = getRequestQueryParameters();

// Check if request is Measurement
if (
  requestPath.toLowerCase() === "/firestore" &&
  getRequestMethod().toUpperCase() === "GET"
) {
  claimRequest();
  if (queryParameters.v != version) {
    log(
      "Wrong client and server versions. Client version is " +
        queryParameters.v +
        " but server version is " +
        version
    );
    return;
  }
  if (!queryParameters.u || queryParameters.u === "undefined") {
    log("User key missed");
    return;
  }

  if (data.referers) {
    const allowedReferers = data.referers.split(",");
    let referer = getRequestHeader("Referer");
    if (!referer) {
      log("Request referer is not set");
      return;
    }
    referer = referer.toLowerCase();
    let refererFound = 0;
    for (let index = 0; index < allowedReferers.length; index++) {
      const allowedReferer = allowedReferers[index].trim().toLowerCase();
      if (referer.indexOf(allowedReferer) > -1) {
        refererFound++;
      }
    }
    if (!refererFound) {
      log("Referer " + referer + " is not set in the list of allowed referers");
      return;
    }
  }

  const origin = getRequestHeader("Origin");
  if (origin) {
    setResponseHeader("Access-Control-Allow-Origin", origin);
    setResponseHeader("Access-Control-Allow-Credentials", "true");
  }

  const prepareRule = (rule) => {
    const params = rule.split(",");
    if (params.length === 3 || params.length === 4) {
      return params.map((param) => decodeUriComponent(param));
    }
    return false;
  };

  let event = {
    event_name: data.eventName,
    user_key: queryParameters.u,
    version: version,
    rules: [],
  };
  event["set_cookie"] = queryParameters.hasOwnProperty("s") ? false : true;
  event["cookie_prefix"] = queryParameters.hasOwnProperty("sp")
    ? queryParameters.sp + "_"
    : "user_";
  if (queryParameters.hasOwnProperty("e")) {
    event["event_type"] = queryParameters.e;
  }
  if (queryParameters.hasOwnProperty("r")) {
    if (getType(queryParameters.r) === "array") {
      queryParameters.r.forEach((rule) => {
        const ruleFormatted = prepareRule(rule);
        if (ruleFormatted) event["rules"].push(ruleFormatted);
      });
    } else if (getType(queryParameters.r) === "string") {
      const ruleFormatted = prepareRule(queryParameters.r);
      if (ruleFormatted) event["rules"].push(ruleFormatted);
    }
  }
  runContainer(event, () => returnResponse());
}


